@model Resturant.ViewModels.MasterServiceModel

@{
    ViewData["Title"] = "Details";
}
<div class="pagetitle">
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Home</a></li>
            <a class="breadcrumb-item" asp-action="Index" asp-controller="MasterService">Master Service</a>
            <a class="breadcrumb-item active" asp-action="Details" asp-controller="MasterService">Details</a>
        </ol>
    </nav>
</div>

<div class="card">
    <div class="card-body">
        <dl class="mt-12 mt-3">
            <dt class="col-sm-12">
                @Html.DisplayNameFor(model => model.MasterServiceTitle)
            </dt>
            <dd class="col-sm-12">
                @Html.DisplayFor(model => model.MasterServiceTitle)
            </dd>
            <dt class="col-sm-12">
                @Html.DisplayNameFor(model => model.MasterServiceDesc)
            </dt>
            <dd class="col-sm-12">
                @Html.DisplayFor(model => model.MasterServiceDesc)
            </dd>
            <dt class="col-sm-12">
                @Html.DisplayNameFor(model => model.MasterServiceImage)
            </dt>
            <dd class="col-sm-12">
                @Html.DisplayFor(model => model.MasterServiceImage)
            </dd>
            <div class="text-end">
                <a asp-action="Index" class="btn btn-success">Back to List</a>

                <a asp-action="Edit" asp-route-id="@Model.MasterServiceId" class="btn btn-primary ">Edit</a> 
            </div>

        </dl>
    </div>
</div>
