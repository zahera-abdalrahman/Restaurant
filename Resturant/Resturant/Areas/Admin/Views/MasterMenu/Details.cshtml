@model Resturant.ViewModels.MasterMenuModel

@{
    ViewData["Title"] = "Details";
}

<div class="pagetitle">
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Home</a></li>
            <a class="breadcrumb-item" asp-action="Index" asp-controller="MasterMenu">Master Menu</a>
            <a class="breadcrumb-item active" asp-action="Details" asp-controller="MasterMenu">Details</a>

        </ol>
    </nav>
</div>


<div class="card">
    <div class="card-body">

        <dl class="mt-3">
            <dt class="col-sm-12">
                @Html.DisplayNameFor(model => model.MasterMenuName)
            </dt>
            <dd class="col-sm-12">
                @Html.DisplayFor(model => model.MasterMenuName)
            </dd>
            <dt class="col-sm-12">
                @Html.DisplayNameFor(model => model.MasterMenuUrl)
            </dt>
            <dd class="col-sm-12">
                @Html.DisplayFor(model => model.MasterMenuUrl)
            </dd>
            <div class="text-end">
                <a asp-action="Index" class="btn btn-success">Back to List</a>

                <a asp-action="Edit" asp-route-id="@Model.MasterMenuId" class="btn btn-primary ">Edit</a>
            </div>
        </dl>
    </div>
</div>

