@model Resturant.ViewModels.TransactionContactUsModel

@{
    ViewData["Title"] = "Create";
}

<div class="pagetitle">
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Home</a></li>
            <a class="breadcrumb-item" asp-action="Index" asp-controller="TransactionContactUs">Transaction ContactUs</a>
            <a class="breadcrumb-item active" asp-action="Create" asp-controller="TransactionContactUs">Create</a>
        </ol>
    </nav>
</div>
<div class="card">
    <div class="card-body">
        <div class="col-md-12">
            <form asp-action="Create" class="mt-3">
                <div asp-validation-summary="ModelOnly" class="row g-3 mt-3" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="TransactionContactUsFullName" class="control-label"></label>
                    <input asp-for="TransactionContactUsFullName" class="form-control" />
                    <span asp-validation-for="TransactionContactUsFullName" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="TransactionContactUsEmail" class="control-label"></label>
                    <input asp-for="TransactionContactUsEmail" class="form-control" />
                    <span asp-validation-for="TransactionContactUsEmail" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="TransactionContactUsSubject" class="control-label"></label>
                    <input asp-for="TransactionContactUsSubject" class="form-control" />
                    <span asp-validation-for="TransactionContactUsSubject" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="TransactionContactUsMessage" class="control-label"></label>
                    <input asp-for="TransactionContactUsMessage" class="form-control" />
                    <span asp-validation-for="TransactionContactUsMessage" class="text-danger"></span>
                </div>
                <div class="text-end">
                    <input type="submit" value="Create" class="btn btn-primary" />
                    <a asp-action="Index" class="btn btn-outline-primary">Back to List</a>
                </div>
               
            </form>
        </div>
      
    </div>
</div>



