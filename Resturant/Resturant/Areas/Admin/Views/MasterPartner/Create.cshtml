@model Resturant.ViewModels.MasterPartnerModel

@{
    ViewData["Title"] = "Create";
}

<div class="pagetitle">
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Home</a></li>
            <a class="breadcrumb-item" asp-action="Index" asp-controller="MasterPartner">Master Partner</a>
            <a class="breadcrumb-item active" asp-action="Create" asp-controller="MasterPartner">Create</a>
        </ol>
    </nav>
</div>
<div class="card">
    <div class="card-body">
        <div class="col-md-12">
            <form asp-action="Create" class="mt-3" enctype="multipart/form-data">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="row mb-3">
                    <div class="col-6">
                        <div class="form-group">
                            <label asp-for="MasterPartnerName" class="control-label"></label>
                            <input asp-for="MasterPartnerName" class="form-control" />
                            <span asp-validation-for="MasterPartnerName" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-6">
                        <div class="form-group">
                            <label asp-for="MasterPartnerWebsiteUrl" class="control-label"></label>
                            <input asp-for="MasterPartnerWebsiteUrl" class="form-control" />
                            <span asp-validation-for="MasterPartnerWebsiteUrl" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div class="row mb-3">
                    <div class="col-6">
                        <div class="form-group">
                            <label asp-for="MasterPartnerLogoImageUrl" class="control-label"></label>
                            <input asp-for="File" type="file" class="form-control" />
                            <span asp-validation-for="MasterPartnerLogoImageUrl" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div class="text-end">
                    <a asp-action="Index" class="btn btn-success">Back to List</a>

                    <input type="submit" value="Create" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>
</div>



