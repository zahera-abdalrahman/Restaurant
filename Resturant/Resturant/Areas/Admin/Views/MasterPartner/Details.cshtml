@model Resturant.ViewModels.MasterPartnerModel

@{
    ViewData["Title"] = "Details";
}

<div class="pagetitle">
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Home</a></li>
            <a class="breadcrumb-item" asp-action="Index" asp-controller="MasterContactUsInformation">Master Partner</a>
            <a class="breadcrumb-item active" asp-action="Details" asp-controller="MasterPartner">Details</a>
        </ol>
    </nav>
</div>


<div class="card">
    <div class="card-body">
        <dl class="mt-3">
            <dt class="col-sm-12">
                @Html.DisplayNameFor(model => model.MasterPartnerName)
            </dt>
            <dd class="col-sm-12">
                @Html.DisplayFor(model => model.MasterPartnerName)
            </dd>
            <dt class="col-sm-12">
                @Html.DisplayNameFor(model => model.MasterPartnerLogoImageUrl)
            </dt>
            <dd class="col-sm-12">
                <img src="~/images/@Model.MasterPartnerLogoImageUrl" style="max-width:60px" />
            </dd>
            <dt class="col-sm-12">
                @Html.DisplayNameFor(model => model.MasterPartnerWebsiteUrl)
            </dt>
            <dd class="col-sm-12">
                @Html.DisplayFor(model => model.MasterPartnerWebsiteUrl)
            </dd>
            <div class="text-end">
                <a asp-action="Index" class="btn btn-success">Back to List</a>

                <a asp-action="Edit" asp-route-id="@Model.MasterPartnerId" class="btn btn-primary">Edit</a>
            </div>
        </dl>
    </div>
</div>

