@model Resturant.ViewModels.MasterSocialMediumModel

@{
    ViewData["Title"] = "Details";
}

<div class="pagetitle">
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Home</a></li>
            <a class="breadcrumb-item" asp-action="Index" asp-controller="MasterSocialMedium">Master Social Media</a>
            <a class="breadcrumb-item active" asp-action="Details" asp-controller="MasterSocialMedium">Details</a>
        </ol>
    </nav>
</div>
<<div class="card">
    <div class="card-body">
        <dl class="mt-3">
            <dt class="col-sm-12">
                @Html.DisplayNameFor(model => model.MasterSocialMediumImageUrl)
            </dt>
            <dd class="col-sm-12">
                @Html.DisplayFor(model => model.MasterSocialMediumImageUrl)
            </dd>
            <dt class="col-sm-12">
                @Html.DisplayNameFor(model => model.MasterSocialMediumUrl)
            </dt>
            <dd class="col-sm-12">
                @Html.DisplayFor(model => model.MasterSocialMediumUrl)
            </dd>
            <dt class="col-sm-12">
                @Html.DisplayNameFor(model => model.MasterSocialMediumName)
            </dt>
            <dd class="col-sm-12">
                @Html.DisplayFor(model => model.MasterSocialMediumName)
            </dd>
            <div class="text-end">
                <a asp-action="Index" class="btn btn-success">Back to List</a>

                <a asp-action="Edit" asp-route-id="@Model.MasterSocialMediumId" class="btn btn-primary ">Edit</a>
            </div>
        </dl>
    </div>
</div>


