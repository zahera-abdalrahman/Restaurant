@model Resturant.ViewModels.Register

@{
    ViewData["Title"] = "UserDetails";
}

<div class="pagetitle">
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Home</a></li>
            <a class="breadcrumb-item" asp-action="ListUsers" asp-controller="Account">User Details</a>
            <a class="breadcrumb-item active" asp-action="UserDetails" asp-controller="Account">Details</a>

        </ol>
    </nav>
</div>
<div class="card">
    <div class="card-body">
        <dl class="mt-3">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Email)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Email)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.UserName)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.UserName)
            </dd>
            <div class="text-end">
                <a asp-action="ListUsers" asp-controller="Account" class="btn btn-success">Back to List</a>

                <a asp-action="EditUser" asp-controller="Account" asp-route-id="@Model.Id" class="btn btn-primary ">Edit</a>
            </div>
        </dl>
    </div>
</div>

