@model Resturant.ViewModels.MasterCategoryMenuModel

@{
    ViewData["Title"] = "Edit";
}

<div class="pagetitle">
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Home</a></li>
            <a class="breadcrumb-item" asp-action="Index" asp-controller="MasterCategoryMenu">Master Category Menu</a>
            <a class="breadcrumb-item active" asp-action="Edit" asp-controller="MasterCategoryMenu">Edit</a>
        </ol>
    </nav>
</div>


<div class="card">
    <div class="card-body">
        <div class="col-md-12">
            <form asp-action="Edit" class="row g-3 mt-3">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="MasterCategoryMenuId" />
                <div class="form-group">
                    <label asp-for="MasterCategoryMenuName" class="control-label"></label>
                    <input asp-for="MasterCategoryMenuName" class="form-control"/>
                    <span asp-validation-for="MasterCategoryMenuName" class="text-danger"></span>
                </div>
                <div class="text-end ">
                    <a asp-action="Index" class="btn btn-success">Back to List</a>
                    <input type="submit" value="Save" class="btn btn-primary" />
                </div>

            </form>
        </div>
       
    </div>
</div>


